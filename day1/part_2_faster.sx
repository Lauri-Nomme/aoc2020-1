LIST('https://raw.githubusercontent.com/spectx/aoc2020/main/day1/input/input.txt')
| parse(pattern:"ARRAY{INT:num EOL}{1,32000}:arr")
| select(n1:unnest(arr))
| select(j:2020-n1)
| filter(j > 0)
| select(n2:unnest(arr))
| select(j:j-n2)
| filter(j > 0)
| select(n3:unnest(arr))
| filter(j = n3)
| select(result:n1*n2*n3)
| limit(1)

